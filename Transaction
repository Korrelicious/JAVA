package MinBankApp;

import java.util.Date;

public class Transaction {

    private int id;
    private String title;
    private Date date;
    private double amount;


    public Transaction(int id,String title,Date date,double amount){
        this.id = id;
        this.title = title;
        this.date = date;
        this.amount = amount;


    }

    @Override
    public String toString() {
        return "[" + id + "] " + date + " - [" + amount + "] kr - " + title ;
    }

    public double getAmount(){
        return amount;
    }

    public int getId(){
        return id;
    }
public Date getDate(){
        return date;
}
}
